
@model WebApplication6.Models.Admin.AdminModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ADMIN</title>
    <link href="~/css/sidebar.css" rel="stylesheet">
    <script src="~/js/jquery-3.2.1.min.js"></script>
    <script src="~/js/jquery.validate.min.js"></script>
    <link href="~/css/Validation.css" media="all" rel="stylesheet" type="text/css" />

    <script>


    </script>
</head>
<body>
    <h1> Admin Panel </h1>
   
        <div class="col-lg-12" style="margin-top:50px; ">
           
             
                <div class="row">
                    <div class="col-lg-12" >
                        <h3>Dashboard</h3>
                      <hr>
                        <div class="col-lg-2">
                           
                                    <ul class="nav nav-tabs tabs-left">
                                        <li class="active">
                                            <a href="#tab_default_1" data-toggle="tab">
                                                Add New Item
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#tab_default_2" data-toggle="tab">
                                                Remove Item
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#tab_default_3" data-toggle="tab">
                                                Update Item
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#tab_default_4" data-toggle="tab">
                                                Add/Remove Manufacturer
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#tab_default_5" data-toggle="tab">
                                                Add/Remove Item Type
                                            </a>
                                        </li>

                                        <li>
                                            <a href="#tab_default_6" data-toggle="tab">
                                                Add/Remove Item Sub Type
                                            </a>
                                        </li>

                                        <li>
                                            <a href="#tab_default_7" data-toggle="tab">
                                                Remove Review
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#tab_default_8" data-toggle="tab">
                                                Update/Remove User
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#tab_default_9" data-toggle="tab">
                                                Add/Remove Slider Images
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                                <div class="col-lg-10">
                                    <div class="tab-content" style="  padding:5px;">
                                        <div class="tab-pane active" id="tab_default_1">
                                            @{await Html.RenderPartialAsync("AddItem"); }
                                        </div>
                                        <div class="tab-pane" id="tab_default_2">
                                            @{await Html.RenderPartialAsync("RemoveItem");}
                                        </div>
                                        <div class="tab-pane" id="tab_default_3">
                                            @{await Html.RenderPartialAsync("UpdateItem");}
                                        </div>
                                        <div class="tab-pane" id="tab_default_4">
                                            @{ await Html.RenderPartialAsync("AddRemoveManufacturer");}
                                        </div>
                                        <div class="tab-pane" id="tab_default_5">
                                            @{ await Html.RenderPartialAsync("AddRemoveItemType");}
                                        </div>
                                        <div class="tab-pane" id="tab_default_6">
                                            @{ await Html.RenderPartialAsync("AddRemoveItemSubType");}
                                        </div>
                                        <div class="tab-pane" id="tab_default_7">
                                            @{ await Html.RenderPartialAsync("RemoveReview");}
                                        </div>
                                        <div class="tab-pane" id="tab_default_8">
                                            @{await Html.RenderPartialAsync("UpdateRemoveUser"); }
                                        </div>
                                        <div class="tab-pane" id="tab_default_9">
                                            @{await Html.RenderPartialAsync("ImageUpload"); }
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                
 
   
</body>
</html>
